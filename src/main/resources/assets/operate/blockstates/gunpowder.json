{
  "multipart": [
    {
      "apply": { "model": "operate:block/dust_dot" }
    },
    {   "when": {"east" :  "true"},
      "apply": { "model": "operate:block/dust_side", "y": 90 }
    },
    {   "when": {"south": "true"},
      "apply": { "model": "operate:block/dust_side", "y": 180 }
    },
    {   "when": {"west":  "true"},
      "apply": { "model": "operate:block/dust_side", "y": 270 }
    },
    {   "when": {"north":  "true"},
      "apply": { "model": "operate:block/dust_side" }
    }
  ]
}